pipeline {
    agent { label 'Jenkins-Agent'}
    tools {
          jdk 'Java17'
          maven 'Maven3'
}
    stages {
        stage("cleanup Workspace") {
            steps {
                cleanWs()
            }
        }
        stage("checkoutfrom SCM") {
            steps {
                git branch: 'main', credentialsID: 'github', https://github.com/Venkoladev/devopsproject-registration-app'
                   }
                                   }
        stage('Build application') {
            steps {
                sh "mvn clean package'
                  }
                                    }
    }
}
